{% extends 'docs.html.twig' %}

{% block title %}Aeon PHP - Documentation{% endblock %}

{% block body %}
    <div class="row doc-content-body">
        <article class="col-md-9">
        <section id="calendar-interface">
            <h5 class="section-title-2">Calendar</h5>
            <hr/>
            <ul>
            {% for phpClass in calendarClasses %}
                <li><a href="{{ path('docs_calendar_class', {'classSlug' : phpClass.slug}) }}">{{ phpClass.shortName }}</a></li>
            {% endfor %}
            </ul>
        </section>
        </article>
    </div>
{% endblock %}
